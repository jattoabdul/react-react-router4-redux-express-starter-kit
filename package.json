{
  "name": "react-login-starter",
  "version": "1.0.0",
  "description": "a react redux starter app",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run start:prod",
    "start:prod": "node server/dist/bin/www",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "build:client": "webpack --config webpack.config.babel.js",
    "build:server": "rimraf server/dist/ && babel ./server --out-dir server/dist/ --copy-files",
    "start:dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "webpack-dev-server --hot",
    "server:dev": "nodemon --watch server --exec babel-node -- server/bin/www"
  },
  "keywords": [
    "react-redux"
  ],
  "author": "abdulqahhar aminujatto <jattoade@gmail.com>",
  "license": "ISC",
